.win_title .text{ display:inline-block; color:#474747; font-size:14px; -webkit-box-reflect: below 1px -webkit-gradient(linear, 0 0, 0 100%, from(transparent), color-stop(.5, transparent), to(rgba(3,3,3,.2)));position: relative;
top: -10px;}
.pwd_input{display:block; height:38px; width:80%; margin:0 10%; border:1px solid #E1E1E1; text-align:center; font-size:14px; box-sizing:border-box; margin-bottom:10px;}
.pwd_btn{ display:block; height:38px; line-height:38px; width:80%; margin:0 10%; background-color:#2A8EE3; text-align:center; color:#fff;}
.pwd_btn2{display:block; height:38px; line-height:38px; width:80%; margin:0 10%; background-color:#D5D5D5; text-align:center; color:#fff;}
.add-text{ display:block; width:100%; line-height:24px; padding:5px 25px; font-family:'Microsoft Yahei','Simsun'; box-sizing:border-box; font-size:12px;}
.pwd_p{color:#FE4541};
